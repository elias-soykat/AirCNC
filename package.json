{
  "name": "aircnc",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --quiet src/server.ts",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "dev:docker:build": "docker compose build --no-cache",
    "dev:docker": "docker compose up",
    "dev:docker:down": "docker compose down --remove-orphans",
    "test": "mocha \"./test/**/*.spec.ts\" --timeout 15000 --exit",
    "test:docker:build": "docker compose --file docker-compose.test.yml build --no-cache",
    "test:docker": "docker compose --file docker-compose.test.yml up --abort-on-container-exit --exit-code-from aircnc",
    "test:docker:down": "docker compose --file docker-compose.test.yml down --volumes --remove-orphans"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.4",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
